{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "algolia/algoliasearch-client-php": "^1.25",
    "fideloper/proxy": "~4.0",
    "laravel/framework": "5.6.*",
    "laravel/passport": "~5.0",
    "laravel/scout": "~4.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "merodiro/friendships": "dev-master",
    "merodiro/settings": "dev-master",
    "merodiro/simple-admin": "dev-master",
    "predis/predis": "^1.1",
    "sentry/sentry-laravel": "^0.8.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.0",
    "filp/whoops": "~2.0",
    "nunomaduro/collision": "~1.1",
    "fzaninotto/faker": "~1.4",
    "jeroen-g/laravel-packager": "^1.6",
    "laravel/tinker": "~1.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "tightenco/mailthief": "^0.3.5",
    "viacreative/sudo-su": "^1.1",
    "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": ["database/seeds", "database/factories"],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": ["@php artisan key:generate"],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "test": "phpunit"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
